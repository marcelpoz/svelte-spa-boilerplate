{
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "singleQuote": true,
  "printWidth": 100,
  "semi": true,
  "trailingComma": "es5",
  "useTabs": false,
  "tabWidth": 2,
  "plugins": ["prettier-plugin-svelte"],
  "svelteAllowShorthand": true,
  "svelteBracketNewLine": true,
  "svelteSortOrder": "markup-scripts-styles",
  "svelteStrictMode": true
}
